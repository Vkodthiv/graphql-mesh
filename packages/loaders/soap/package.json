{
  "name": "@omnigraph/soap",
  "version": "0.0.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "graphql-compose": "9.0.4",
    "fast-xml-parser": "3.21.1",
    "@graphql-mesh/utils": "0.21.1",
    "graphql-scalars": "1.13.1"
  },
  "devDependencies": {
    "cross-undici-fetch": "0.0.20"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
